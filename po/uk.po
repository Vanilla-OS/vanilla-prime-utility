msgid "No description"
msgstr "Без опису"

#: vanilla_prime_utility/gtk/help-overlay.ui:11
msgctxt "shortcut window"
msgid "General"
msgstr "Загальний"

#: vanilla_prime_utility/gtk/help-overlay.ui:14
